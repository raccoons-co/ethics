{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "eol-last": [
      "warn",
      "always"
    ],
    "indent": [
      "warn",
      4
    ],
    "max-len": [
      "warn",
      120
    ],
    "max-lines": [
      "warn",
      92
    ],
    "no-unused-vars": [
      "error",
      {
        "args": "all"
      }
    ],
    "quotes": [
      "warn",
      "double"
    ],
    "semi": "off",
    "@typescript-eslint/semi": "warn"
  },
  "reportUnusedDisableDirectives": true,
  "noInlineConfig": false
}
